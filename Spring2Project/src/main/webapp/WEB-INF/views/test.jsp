        // JK-15618 IE 지원 종료 레이어 팝업 노출 작업 요청
        $(function () {
            var cookieHandler = cookieHandler || {};
            cookieHandler = {
                set_cookie: function (cookie_name, value, days, path) {
                    path = typeof path === 'undefined' ? '/' : path;
                    var exdate = new Date();
                    exdate.setDate(exdate.getDate() + days);

                    // 설정일수만큼 현재 시간에 만료값으로 지정
                    var cookie_value = escape(value) + ((days === null) ? '' : '; expires=' + exdate.toUTCString()) + ';path=' + path;
                    document.cookie = cookie_name + '=' + cookie_value;
                },
                get_cookie: function (name) {
                    var x, y;
                    var val = document.cookie.split(';');

                    for (var i = 0; i < val.length; i++) {
                        x = val[i].substr(0, val[i].indexOf('='));
                        y = val[i].substr(val[i].indexOf('=') + 1);

                        // 앞과 뒤의 공백 제거하기
                        x = x.replace(/^\s+|\s+$/g, '');
                        if (x === name) {
                            // unescape로 디코딩 후, 값 리턴
                            return unescape(y);
                        }
                    }
                },
                _delCookie: function (name) {
                    var expireDate = new Date();
                    expireDate.setDate(expireDate.getDate() - 1);
                    document.cookie = name + '=' + '; expires=' + expireDate.toGMTString() + '; path=/';
                }
            };

            if (7 == 2) {
                var userIEGuideLayer = {
                    load: function () {
                        var self = this;
                        self.services.closeIEGuideStatus();

                        $("#closeIEGuide7Day").on("click", self.events.onClickCloseIEGuide7Days);
                        $("#closeIEGuide").on("click", self.events.onClickCloseIIEGuideLayer);
                    },
                    events: {
                        onClickCloseIEGuide7Days: function () {
                            userIEGuideLayer.methods.hideIEGuideLayer7Days();
                        },
                        onClickCloseIIEGuideLayer: function () {
                            userIEGuideLayer.methods.hideIEGuideLayer();
                        }
                    },
                    methods: {
                        showIEGuideLayer: function () {
                            $(".ie-end").show();
                        },
                        hideIEGuideLayer7Days: function () {
                            if ($("#closeIEGuide7Day").is(":checked")) {
                                cookieHandler.set_cookie("IEGuideLayerView", 1, 7, "");
                            }
                            else {
                                cookieHandler._delCookie("IEGuideLayerView");
                            }
                        },
                        hideIEGuideLayer: function () {
                            if (!$("#closeIEGuide7Day").is(":checked")) {
                                cookieHandler.set_cookie("IEGuideLayerView", 1, 1, "");
                            }

                            $(".ie-end").hide();
                        }
                    },
                    services: {
                        closeIEGuideStatus: function () {
                            if (!cookieHandler.get_cookie("IEGuideLayerView")) {
                                userIEGuideLayer.methods.showIEGuideLayer();
                            }
                        }
                    }
                };

                userIEGuideLayer.load();
            }

            var systemCheckLayer = {
                load: function () {
                    var sDate = new Date();
                    var eDate = new Date('2024/2/23 01:00:00');

                    var diff_edate_sdate = eDate.getTime() - sDate.getTime();
                    var diffday = diff_edate_sdate / (1000 * 60 * 60 * 24);
                    var self = this;
                    self.services.closeSystemCheckStatus(diffday);

                    $("#closeSystemCheck").on("click", self.events.onClickCloseSystemCheckLayer);
                    $("#layerSystemCheck").on("click", self.events.onClickCloseGuide7Days);
                    $(".systemcheckdetail").on("click", self.events.onClickNoticeButton);
                },
                events: {
                    onClickCloseSystemCheckLayer: function () {
                        systemCheckLayer.methods.hideSystemCheckLayer(false);
                    },
                    onClickCloseGuide7Days: function () {
                        systemCheckLayer.methods.hideSystemCheckLayer(true);
                    },
                    onClickNoticeButton: function () {
                        location.href = "/help/notice/view?no=14756";
                    }
                },
                methods: {
                    showSystemCheckLayer: function () {
                        $(".checksystem").show();
                        $(".checksystem").addClass("attached");
                    },
                    hideSystemCheckLayer: function (cVal) {
                        if (cVal) {
                            cookieHandler.set_cookie("SystemCheckLayerView2402", 1, 10, "");
                        }
                        $(".checksystem").hide();
                        $(".checksystem").removeClass("attached");
                    }
                },
                services: {
                    closeSystemCheckStatus: function (diffday) {
                        if (!cookieHandler.get_cookie("SystemCheckLayerView2402") && diffday > 0) {
                            systemCheckLayer.methods.showSystemCheckLayer();
                        }
                    }
                }
            };

            systemCheckLayer.load();
        });

