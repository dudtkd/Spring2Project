    }

    // Braze - 어트리뷰트 데이터 타입
    function SetAttributeDataType(ATTR_TYPE) {
        var DATA_TYPE = "string";

        switch (ATTR_TYPE) {
            case 1:
            case 2:
            case 3:
            case 200:
            case 201:
            case 202:
            case 203:
            case 204:
            case 205:
                DATA_TYPE = "array";
                break;
            case 231:
                DATA_TYPE = "int";
                break;
            default:
                break;
        }

        return DATA_TYPE;
    }

    // Braze - 어트리뷰트 값
    function SetAttributePropertyByType(ATTR_TYPE, PROP_STRING) {
        var result = "";
        var date = new Date();

        switch (ATTR_TYPE) {
            case 1:
                result = jk.util.string.format("지원일자_{0}/기업ID_{1}/기업명_{2}/공고ID_{3}/지원타입_{4}"
                    , date.yyyymmdd()
                    , PROP_STRING.split("|")[2]
                    , PROP_STRING.split("|")[1]
                    , PROP_STRING.split("|")[4]
                    , "즉시지원"
                )
                break;
            case 2:
                result = jk.util.string.format("스크랩일자_{0}/기업ID_{1}/기업명_{2}/공고ID_{3}/지원타입_{4}"
                    , date.yyyymmdd()
                    , PROP_STRING.split("|")[6]
                    , PROP_STRING.split("|")[5]
                    , PROP_STRING.split("|")[1]
                    , "즉시지원"
                )
                break;
            case 3:
                result = jk.util.string.format("취소일자_{0}/기업ID_{1}/기업명_{2}/공고ID_{3}/지원타입_{4}"
                    , date.yyyymmdd()
                    , PROP_STRING.split("|")[0]
                    , PROP_STRING.split("|")[1]
                    , PROP_STRING.split("|")[2]
                    , "즉시지원"
                )
                break;
            case 4:
            case 5:
            case 6:
            case 7:
            case 8:
            case 9:
            case 10:
            case 11:
            case 12:
            case 13:
            case 14:
            case 15:
            case 16:
            case 17:
            case 200:
            case 201:
            case 202:
            case 203:
            case 204:
            case 205:
            case 210:
                result = PROP_STRING;
                break;
            default:
                result = PROP_STRING;
                break;
        }

        return result;
    }

    Date.prototype.yyyymmdd = function () {
        var mm = this.getMonth() + 1; // getMonth() is zero-based
        var dd = this.getDate();

        return [this.getFullYear(),
        (mm > 9 ? '' : '0') + mm,
        (dd > 9 ? '' : '0') + dd,
        (this.getHours() > 9 ? '' : '0') + this.getHours(),
        (this.getMinutes() > 9 ? '' : '0') + this.getMinutes(),
        (this.getSeconds() > 9 ? '' : '0') + this.getSeconds()
        ].join('');
    };

    // yyyy-MM-dd
    Date.prototype.simple_yyyymmdd = function () {
        var mm = this.getMonth() + 1; // getMonth() is zero-based
        var dd = this.getDate();

        return [this.getFullYear(),
        (mm > 9 ? '' : '0') + mm,
        (dd > 9 ? '' : '0') + dd
        ].join('-');
    };



</script>
